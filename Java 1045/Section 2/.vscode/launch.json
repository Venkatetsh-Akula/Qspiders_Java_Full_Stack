        public class ruf{
            public static void main(String[] args){
                int[] arr={4,1,1,1,2,3,5};
                int i=0,j=0,sum=0,ans=100,target=2;
                while(j<arr.length){
                    if(sum<target){
                        sum+=arr[j]; //(4,1) ()
                        j++;
                    }
                    else if(sum==target){
                        ans=Math.min(ans,j-i);  //1 
                        sum-=arr[i];  //1
                        i++;
                    }
                    else if(sum>target){
                        sum-=arr[i];
                        i++;
                    }
                }
                System.out.println(ans);
            }
        }

